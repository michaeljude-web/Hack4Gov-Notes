C:\home\kali\Downloads> import hashlib
import itertools
import string

correct_hash = open("level5.hash.bin", "rb").read()

alphabet = string.ascii_letters + string.digits

for length in range(1, 6):  # try pw length 1–5
    for pw in itertools.product(alphabet, repeat=length):
        pw = "".join(pw)
        if hashlib.md5(pw.encode()).digest() == correct_hash:
            print("FOUND PASSWORD:", pw)
            exit()

